{
  "name": "@onlymoons-io/networks",
  "version": "0.0.14",
  "description": "List of networks used within onlymoons-io products and services",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">= 18"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "pinst": "^3.0.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "tsutils": "~3.21",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "clean": "rimraf pages dist tmp",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "build:release": "npm run clean && tsc -p tsconfig.release.json",
    "lint": "eslint . --ext .ts --ext .mts",
    "test": "jest",
    "test:cov": "npm run test --coverage",
    "test:watch": "npm run test --watch",
    "prettier": "prettier --config .prettierrc.json --write .",
    "prepare": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "author": "onlymoons-io",
  "license": "GPL-3.0+",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onlymoons-io/networks.git"
  },
  "bugs": "https://github.com/onlymoons-io/networks/issues",
  "homepage": "https://onlymoons.io/"
}
